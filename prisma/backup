// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/db.db"
}

model USERS {
  FirstName     String
  LastName      String
  Following     String
  Followes      String
  QuizMade      String
  QuizAttempted String
  Score         Int
  ProfilePic    String
  Bio           String
  Username      String @id
  Password      String
}

model SESSIONS {
  sessioncookie String @id
}